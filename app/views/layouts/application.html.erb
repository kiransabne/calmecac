<!DOCTYPE html>
<html>
  <head>
    <title>Calmecac</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'shared/header' %>
    <%= yield %>
    <% flash.each do |key, value| %>
      <%= render '/shared/flash', key: key, value: value %>
    <% end %>
  </body>
</html>
